{
    "name": "react-enterprise-starter",
    "version": "0.1.0",
    "description": "React application ready to work in enterprise environment",
    "main": "index.js",
    "scripts": {
        "build": "webpack",
        "start": "npm run build && node build/server.js",
        "dev": "nodemon -e ts,tsx,json --exec \"npm run start\"",
        "lint": "tslint --project tsconfig.json",
        "test": "npm run test:unit && npm run test:automated",
        "test:unit": "jest --config ./jest.config.js",
        "test:automated": "tsc -p tsconfig.bdd.json && node build/bdd"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/grzegorzjudas/react-enterprise-starter.git"
    },
    "keywords": [
        "react",
        "enterprise",
        "starter"
    ],
    "author": "Grzegorz Judas <grzegorz.judas@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/grzegorzjudas/react-enterprise-starter/issues"
    },
    "homepage": "https://github.com/grzegorzjudas/react-enterprise-starter#readme",
    "dependencies": {
        "body-parser": "^1.18.3",
        "compression": "^1.7.3",
        "express": "^4.16.4",
        "joi": "^14.3.1",
        "react": "^16.8.2",
        "react-dom": "^16.8.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/compression": "^0.0.36",
        "@types/enzyme": "^3.9.0",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/express": "^4.16.1",
        "@types/jest": "^24.0.9",
        "@types/joi": "^14.3.1",
        "@types/puppeteer": "^1.12.3",
        "@types/react": "^16.8.4",
        "@types/react-dom": "^16.8.2",
        "css-loader": "^2.1.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "html-loader": "^0.5.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.1.0",
        "nodemon": "^1.18.10",
        "puppeteer": "^1.13.0",
        "style-loader": "^0.23.1",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "tslint": "^5.13.0",
        "tslint-config-standard": "^8.0.1",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.3.3",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3"
    }
}
